<div class="c-product-media__nav swiper">
    <div class="swiper-wrapper">
        {% for y in 1..3 %}
            {% for i in 1..3 %}
                <div class="swiper-slide">
                    <picture>
                        <source srcset="../img/demo/product/{{ i }}.png, ../img/demo/product/{{ i }}@2x.png 2x" type="image/png">
                        <img src="../img/demo/product/{{ i }}.png" width="98" height="148" alt="">
                    </picture>
                </div>
            {% endfor %}
        {% endfor %}
    </div>
</div>
